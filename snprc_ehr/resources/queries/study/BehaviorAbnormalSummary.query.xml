<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="BehaviorAbnormalSummary" tableDbType="TABLE">
                <tableTitle>Abnormal Behavior Detail</tableTitle>
                <columns>
                    <column columnName="Id">
                    </column>
                    <column columnName="BehaviorId">
                        <columnTitle>Behavior Id</columnTitle>
                    </column>
                    <column columnName="FirstReported">
                        <columnTitle>First Reported</columnTitle>
                        <formatString>Date</formatString>
                    </column>
                    <column columnName="LastReported">
                        <columnTitle>Last Reported</columnTitle>
                        <formatString>Date</formatString>
                    </column>
                    <column columnName="Behavior">
                        <columnTitle>Behavior Name</columnTitle>
                    </column>
                    <column columnName="BehaviorDesc">
                        <columnTitle>Behavior Description</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="Incidents">
                        <columnTitle>Incidents of behavior</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
