<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="blood" tableDbType="TABLE" useColumnOrder="true">
                <tableUrl />
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="lsid">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="Id">
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>Id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="date"/>
                    <column columnName="blood_volume::Values">
                        <columnTitle>Blood Volume</columnTitle>
                        <measure>true</measure>
                    </column>
                    <column columnName="reason::Values" >
                        <columnTitle>Reason</columnTitle>
                    </column>
                    <column columnName="IACUC"/>
                </columns>
                <tableTitle>Blood Draws</tableTitle>
            </table>
        </tables>
    </metadata>
</query>
