<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsParentStatus" tableDbType="NOT_IN_DB">
                <tableTitle>Parents</tableTitle>
                <columns>
                    <column columnName="id">
                        <columnTitle>Offspring Id</columnTitle>
                    </column>
                    <column columnName="Parent_id">
                        <columnTitle>Parent Id</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="calculated_status">
                        <columnTitle>Status</columnTitle>
                    </column>
                    <column columnName="totalOffspring">
                        <columnTitle>Number of Offspring</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
