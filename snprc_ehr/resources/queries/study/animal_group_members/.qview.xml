<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date">
            <properties>
                <property name="columnTitle" value = "Start date"/>
            </properties>
        </column>
        <column name="enddate">
            <properties>
                <property name="columnTitle" value = "End date"/>
            </properties>
        </column>
        <column name="groupId/category_code/description">
            <properties>
                <property name="columnTitle" value="Category"/>
            </properties>
        </column>
        <column name="groupId">
            <properties>
                <property name="columnTitle" value="Group"/>
            </properties>
        </column>
        <column name="modifiedBy">
            <properties>
                <property name="columnTitle" value="Modified by"/>
            </properties>
        </column>
        <column name="modified">
            <properties>
                <property name="columnTitle" value="Date last modified"/>
            </properties>
        </column>
        <column name="isActive">
            <properties>
                <property name="columnTitle" value="Is Active?"/>
            </properties>
        </column>

    </columns>
    <sorts>
        <sort column="id" descending="false" />
        <sort column="Date" descending="true" />
    </sorts>
</customView>
