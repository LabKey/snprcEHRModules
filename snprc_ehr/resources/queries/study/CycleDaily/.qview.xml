<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date">
            <properties>
                <property name="columnTitle" value="Cycle Date"/>
            </properties>
        </column>
        <column name="TumescenceIndex">
            <properties>
                <property name = "columnTitle" value="TI"/>
            </properties>
        </column>
        <column name="VaginalBleeding">
            <properties>
                <property name = "columnTitle" value="VI"/>
            </properties>
        </column>
        <column name="PurpleColor">
            <properties>
                <property name = "columnTitle" value="PC"/>
            </properties>
        </column>
        <column name="CarryingInfant">
            <properties>
                <property name = "columnTitle" value="CI"/>
            </properties>
        </column>
        <column name="MaleStatus"/>
        <column name="MaleId"/>
        <column name="CycleLocation"/>
        <column name="modifiedby">
            <properties>
                <property name="columnTitle" value="User Name"/>
            </properties>
        </column>
    </columns>
    <sorts>
        <sort column="date" descending="true" />
        <sort column="Id" descending="false"/>
    </sorts>
</customView>