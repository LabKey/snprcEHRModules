<query xmlns="http://labkey.org/data/xml/query">
<metadata>
    <tables xmlns="http://labkey.org/data/xml">
        <table tableName="demographicsMostRecentPhysicalDate" tableDbType="NOT_IN_DB">
            <tableTitle>Most Recent Physical Date</tableTitle>
            <columns>
                <column columnName="Id">
                    <isKeyField>true</isKeyField>
                    <isHidden>true</isHidden>
                </column>
                <column columnName="MostRecentPhysicalDate">
                    <formatString>MM-dd-yy</formatString>
                    <url>/query/executeQuery.view?schemaName=study&amp;
                        query.queryName=encounters&amp;
                        query.Id~eq=${Id}&amp;
                        query.Date~eq=${MostRecentPhysicalDate}
                    </url>
                    <columnTitle>Most Recent Physical Date</columnTitle>
                </column>
                <column columnName="DaysSinceLastPhysical">
                    <columnTitle>Days Since Last Physical</columnTitle>
                </column>
                <column columnName="DaysUntilDue">
                    <columnTitle>Days Until Due</columnTitle>
                </column>
                <column columnName="MonthsSinceLastPhysical">
                    <columnTitle>Months Since Last Physical</columnTitle>
                </column>
                <column columnName="MonthsUntilDue">
                    <columnTitle>Months Until Due</columnTitle>
                </column>
                <column columnName="PhyStatus">
                    <columnTitle>Physical Status</columnTitle>
                </column>
            </columns>
            <titleColumn>MostRecentPhysicalDate</titleColumn>
        </table>
    </tables>
</metadata>
</query>
