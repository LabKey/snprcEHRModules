<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Departures</name>
    <description>Departure data</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="snprcSrc" queryName="v_departure" />
            <destination schemaName="study" queryName="departure" bulkLoad="true" transactionSize="500" />
        </transform>
    </transforms>
    <incrementalFilter className="SelectAllFilterStrategy"/>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>