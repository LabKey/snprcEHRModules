<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Animal events (encounters)</name>
    <description>Animal encounters data</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="snprcSrc" queryName="animal_event_narratives" />
            <destination schemaName="study" queryName="encounters" bulkLoad="true" transactionSize="500" targetOption="merge"/>
        </transform>
    </transforms>
    <incrementalFilter className="SelectAllFilterStrategy"/>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>