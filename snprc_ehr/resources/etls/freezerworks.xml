<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Freezerworks</name>
    <description>Freezerworks Data</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="snprcSrc" queryName="freezerworks" />
            <destination schemaName="study" queryName="Freezerworks" bulkLoad="true" batchSize="500" targetOption="merge"/>
        </transform>
    </transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="timestamp">
        <deletedRowsSource schemaName="snprcSrc" queryName="V_DELETE_FREEZERWORKS" timestampColumnName="entry_date_tm" deletedSourceKeyColumnName="OBJECT_ID" targetKeyColumnName="object_id"/>
    </incrementalFilter>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>