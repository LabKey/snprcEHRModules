<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Labwork Results (queue updates)</name>
<description>Queue up labwork ETLs</description>
<transforms>
    <transform id="step 1" type="TaskrefTransformStep">
        <taskref ref="org.labkey.di.steps.QueueJobTask">
            <settings>
                <setting name="transformId" value="{SNPRC_EHR}/labworkChemistryResults"/>
            </settings>
        </taskref>
    </transform>
    <transform id="step 2" type="TaskrefTransformStep">
        <taskref ref="org.labkey.di.steps.QueueJobTask">
            <settings>
                <setting name="transformId" value="{SNPRC_EHR}/labworkHematologyResults"/>
            </settings>
        </taskref>
    </transform>
    <transform id="step 3" type="TaskrefTransformStep">
        <taskref ref="org.labkey.di.steps.QueueJobTask">
            <settings>
                <setting name="transformId" value="{SNPRC_EHR}/labworkSurveillanceResults"/>
            </settings>
        </taskref>
    </transform>
</transforms>
<schedule>
    <poll interval="60m" />
</schedule>
</etl>