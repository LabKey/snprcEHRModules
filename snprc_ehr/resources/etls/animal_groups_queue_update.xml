<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Animal group (queue updates)</name>
    <description>Queue up animal group ETLs</description>
    <transforms>
            <transform id="step 1" type="TaskrefTransformStep">
                <taskref ref="org.labkey.di.steps.QueueJobTask">
                    <settings>
                        <setting name="transformId" value="{snprc_ehr}/animal_groups"/>
                    </settings>
                </taskref>
            </transform>
        <transform id="step 2" type="TaskrefTransformStep">
            <taskref ref="org.labkey.di.steps.QueueJobTask">
                <settings>
                    <setting name="transformId" value="{snprc_ehr}/animal_group_members"/>
                </settings>
            </taskref>
        </transform>
    </transforms>
    <schedule>
        <poll interval="60m" />
    </schedule>

</etl>
