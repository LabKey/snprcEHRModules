<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Labwork Chemistry Results</name>
<description>Clinical Pathology Results</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="snprcSrc" queryName="v_labwork_chemistry_results" />
        <destination schemaName="study" queryName="labworkResults" bulkLoad="true" transactionSize="500" targetOption="merge"/>
    </transform>
</transforms>
<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="timestamp">
    <deletedRowsSource schemaName="snprcSrc" queryName="v_delete_labwork_results" timestampColumnName="entry_date_tm" deletedSourceKeyColumnName="message_id" targetKeyColumnName="runid"/>
</incrementalFilter>
<schedule>
    <poll interval="60m" />
</schedule>
</etl>
