<tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\schemas\tableInfo.xsd" xmlns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 <table tableName="package" tableDbType="TABLE" useColumnOrder="true">
  <tableTitle>Package</tableTitle>
  <pkColumnName>id</pkColumnName>
  <columns>
   <column columnName="id">
    <isKeyField>true</isKeyField>
   </column>
   <column columnName="name">
    <nullable>true</nullable>
   </column>
   <column columnName="description">
    <nullable>true</nullable>
   </column>
   <column columnName="container">
    <nullable>false</nullable>
   </column>
   <column columnName="objectid">
    <nullable>false</nullable>
   </column>
   <column columnName="Created"/>
   <column columnName="CreatedBy"/>
   <column columnName="Modified"/>
   <column columnName="ModifiedBy"/>
  </columns>
 </table>
 <table tableName="package_category" tableDbType="TABLE" useColumnOrder="true">
  <tableTitle>Package Category</tableTitle>
  <pkColumnName>id</pkColumnName>
  <columns>
   <column columnName="id">
    <isKeyField>true</isKeyField>
   </column>
   <column columnName="description">
    <nullable>true</nullable>
   </column>
   <column columnName="container">
    <nullable>false</nullable>
   </column>
   <column columnName="objectid">
    <nullable>false</nullable>
   </column>
   <column columnName="Created"/>
   <column columnName="CreatedBy"/>
   <column columnName="Modified"/>
   <column columnName="ModifiedBy"/>
  </columns>
 </table>
 <table tableName="package_category_junction" tableDbType="TABLE" useColumnOrder="true">
  <tableTitle>Package Category Junction</tableTitle>
  <pkColumnName>rowId</pkColumnName>
  <columns>
   <column columnName="rowId">
    <isKeyField>true</isKeyField>
   </column>
   <column columnName="packageId">
    <nullable>false</nullable>
    <fk>
      <fkDbSchema>snprc_ehr</fkDbSchema>
      <fkTable>package</fkTable>
      <fkColumnName>id</fkColumnName>
    </fk>
   </column>
   <column columnName="categoryId">
    <nullable>false</nullable>
    <fk>
      <fkDbSchema>snprc_ehr</fkDbSchema>
      <fkTable>package_category</fkTable>
      <fkColumnName>id</fkColumnName>
    </fk>
   </column>
   <column columnName="objectid">
    <nullable>false</nullable>
   </column>
   <column columnName="Created"/>
   <column columnName="CreatedBy"/>
   <column columnName="Modified"/>
   <column columnName="ModifiedBy"/>
  </columns>
 </table>
 <table tableName="species" tableDbType="TABLE" useColumnOrder="true">
  <tableTitle>Species</tableTitle>
  <importUrl></importUrl>
  <insertUrl></insertUrl>
  <updateUrl></updateUrl>
  <deleteUrl></deleteUrl>
  <pkColumnName>species_code</pkColumnName>
  <columns>
   <column columnName="species_code">
    <columnTitle>Species Code (3 char)</columnTitle>
    <description>This is the three character species code that includes species and hybrid information</description>
   </column>
   <column columnName="common">
    <columnTitle>Common name</columnTitle>
    <description>This is the common name for the species, which includes hybrid descriptions.</description>
    <displayWidth>100</displayWidth>
   </column>
   <column columnName="scientific_name">
    <description>This is the scientific name for the species, which includes hybrid descriptions.</description>
     <nullable>true</nullable>
   </column>
   <column columnName="id_prefix">
    <nullable>true</nullable>
   </column>
   <column columnName="mhc_prefix">
    <nullable>true</nullable>
   </column>
   <column columnName="blood_per_kg">
    <columnTitle>Max Blood Draw (mL/kg)</columnTitle>
    <description>This is the maximum blood that can be drawn per kg</description>
   </column>
   <column columnName="max_draw_pct">
    <columnTitle>Max Percent Blood Drawn</columnTitle>
    <description>This is the maximum percent of the animal's blood that can be drawn (expressed as a
     decimal, where 1 indicates 100%)
    </description>
   </column>
   <column columnName="blood_draw_interval">
    <columnTitle>Blood Reconstitution Interval (Days)</columnTitle>
    <description>This is the number of days that must elapse to full reconstitute from a max blood draw
    </description>
   </column>
   <column columnName="cites_code">
    <columnTitle>Cites Code</columnTitle>
   </column>
   <column columnName="dateDisabled">
    <columnTitle>Date Disabled</columnTitle>
   </column>
   <column columnName="arc_species_code">
    <columnTitle>Species Code (2 char)</columnTitle>
    <description>This is the two character species code that is used by the IACUC to group like species.</description>
     <fk>
      <fkDbSchema>ehr_lookups</fkDbSchema>
      <fkTable>species_codes</fkTable>
      <fkColumnName>code</fkColumnName>
     </fk>
   </column>
   <column columnName="objectid" />
   <column columnName="tid" />
  </columns>
 </table>
 <table tableName="clinical_observation_datasets" tableDbType="TABLE" useColumnOrder="true">
  <tableTitle>Clinical Observation Datasets</tableTitle>
  <importUrl></importUrl>
  <insertUrl></insertUrl>
  <updateUrl></updateUrl>
  <deleteUrl></deleteUrl>
  <pkColumnName>rowId</pkColumnName>
  <columns>
   <column columnName="rowId">
    <isKeyField>true</isKeyField>
    </column>
   <column columnName="dataset_name">
    <columnTitle>Dataset Name</columnTitle>
    <nullable>false</nullable>
    <description>Determines which dataset the row belongs to (e.g. cycle_data, daily_observations)</description>
   </column>
   <column columnName="category_name">
    <columnTitle>Category Name</columnTitle>
    <nullable>false</nullable>
    <description>The category to which the data belongs (e.g. cycle data includes 'tumescence_index', 'vaginal_bleeding', 'purple_color', etc.)</description>
   </column>
   <column columnName="sort_order">
      <columnTitle>Sort Order</columnTitle>
      <nullable>true</nullable>
   </column>
   <column columnName="container">
    <nullable>false</nullable>
   </column>
  </columns>
 </table>
</tables>