{
  "name": "snprc_ehr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "npm install",
    "build": "npm run clean && better-npm-run build:prod",
    "start": "better-npm-run build:watch",
    "build-prod": "npm run build",
    "clean": "better-npm-run clean",
    "test": "cross-env NODE_ENV=test jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest --runInBand",
    "teamcity": "better-npm-run build:jest-teamcity",
    "lint": "better-npm-run lint",
    "lint-fix": "better-npm-run lint-fix"
  },
  "test:debug-notes": "See https://jestjs.io/docs/en/asynchronous before running test:debug - in chrome navigate to chrome://inspect",
  "betterScripts": {
    "build:watch": {
      "command": "webpack-dev-server --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack --config webpack/prod.config.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf resources/web/snprc_ehr/gen && rimraf resources/views/gen"
    },
    "build:jest-teamcity": {
      "command": "jest --testResultsProcessor=jest-teamcity-reporter",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "lint": {
      "command": "eslint --ext .jsx,.js src/client/"
    },
    "lint-fix": {
      "command": "eslint --ext .jsx,.js --fix src/client/"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@labkey/api": "1.10.0",
    "@labkey/components": "2.154.1",
    "moment": "^2.26.0",
    "react": "^16.13.1",
    "react-bootstrap": "^0.33.1",
    "react-datepicker": "^2.16.0",
    "react-dom": "^16.13.1",
    "react-router": "^3.2.6",
    "react-select": "^3.1.0",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~12.1.5",
    "@testing-library/user-event": "~12.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/jsdom": "^16.2.4",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-bootstrap": "^0.32.22",
    "@types/react-datepicker": "^2.11.1",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^3.0.23",
    "@types/react-select": "^3.0.21",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "better-npm-run": "0.1.1",
    "bootstrap-sass": "3.4.1",
    "cross-env": "5.2.1",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-async-helpers": "^0.9.1",
    "enzyme-to-json": "3.5.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-html": "^6.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-teamcity-reporter": "^0.9.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^6.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "^2.1.1",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "6.0.2",
    "typescript": "^3.9.5",
    "url-loader": "1.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testRegex": "(\\.(test|spec))\\.([jt]sx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
